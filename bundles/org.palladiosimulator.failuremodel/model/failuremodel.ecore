<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="failuremodel" nsURI="http://palladiosimulator.org/failuresmodel/1.0"
    nsPrefix="failuremodel">
  <eSubpackages name="failurescenario" nsURI="http://palladiosimulator.org/failurescenario/1.0"
      nsPrefix="failurescenario">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="validationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL"/>
    </eAnnotations>
    <eClassifiers xsi:type="ecore:EClass" name="FailureScenarioRepository" eSuperTypes="../../org.palladiosimulator.pcm/model/pcm.ecore#//core/entity/Entity">
      <eStructuralFeatures xsi:type="ecore:EReference" name="failurescenarios" upperBound="-1"
          eType="#//failurescenario/FailureScenario" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="FailureScenario" eSuperTypes="../../org.palladiosimulator.pcm/model/pcm.ecore#//core/entity/Entity">
      <eStructuralFeatures xsi:type="ecore:EReference" name="occurences" upperBound="-1"
          eType="#//failurescenario/Occurence" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Occurence">
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
        <details key="constraints" value="ensureCorrectOrigin"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL">
        <details key="ensureCorrectOrigin" value="( self.origin.oclIsTypeOf(LinkingResourceReference) and ( self.failure.oclIsKindOf(failuremodel::failuretype::LinkFailure) ) )&#xA;or&#xA;( self.origin.oclIsTypeOf(ProcessingResourceReference) and ( self.failure.oclIsKindOf(failuremodel::failuretype::HWFailure) ) )&#xA;or&#xA;( self.origin.oclIsTypeOf(InternalActionReference) and ( self.failure.oclIsKindOf(failuremodel::failuretype::SWFailure) ) )"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="pointOfTime" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="failure" lowerBound="1"
          eType="#//failuretype/Failure"/>
      <eStructuralFeatures xsi:type="ecore:EReference" name="origin" lowerBound="1"
          eType="#//failurescenario/Reference" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Reference" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="LinkingResourceReference" eSuperTypes="#//failurescenario/Reference">
      <eStructuralFeatures xsi:type="ecore:EReference" name="linkingResource" eType="ecore:EClass ../../org.palladiosimulator.pcm/model/pcm.ecore#//resourceenvironment/CommunicationLinkResourceSpecification"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="InternalActionReference" eSuperTypes="#//failurescenario/Reference">
      <eStructuralFeatures xsi:type="ecore:EReference" name="internalAction" lowerBound="1"
          eType="ecore:EClass ../../org.palladiosimulator.pcm/model/pcm.ecore#//seff/InternalAction"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="ProcessingResourceReference" eSuperTypes="#//failurescenario/Reference">
      <eStructuralFeatures xsi:type="ecore:EReference" name="processingResource" lowerBound="1"
          eType="ecore:EClass ../../org.palladiosimulator.pcm/model/pcm.ecore#//resourceenvironment/ProcessingResourceSpecification"/>
    </eClassifiers>
  </eSubpackages>
  <eSubpackages name="failuretype" nsURI="http://palladiosimulator.org/failuretype/1.0"
      nsPrefix="failuretype">
    <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
      <details key="validationDelegates" value="http://www.eclipse.org/emf/2002/Ecore/OCL"/>
    </eAnnotations>
    <eClassifiers xsi:type="ecore:EClass" name="FailureTypeRepository" eSuperTypes="../../org.palladiosimulator.pcm/model/pcm.ecore#//core/entity/Entity">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="In this repository all failures can be created to use them later in a scenario."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="failuretypes" upperBound="-1"
          eType="#//failuretype/Failure" containment="true"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Failure" abstract="true" eSuperTypes="../../org.palladiosimulator.pcm/model/pcm.ecore#//core/entity/Entity">
      <eOperations name="getSupportedElementType" lowerBound="1" upperBound="-1" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SWFailure" abstract="true" eSuperTypes="#//failuretype/Failure">
      <eOperations name="getSupportedElementType" lowerBound="1" upperBound="-1" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="HWFailure" abstract="true" eSuperTypes="#//failuretype/Failure">
      <eOperations name="getSupportedElementType" lowerBound="1" upperBound="-1" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LinkFailure" abstract="true" eSuperTypes="#//failuretype/Failure">
      <eOperations name="getSupportedElementType" lowerBound="1" upperBound="-1" eType="ecore:EClass http://www.eclipse.org/emf/2002/Ecore#//EObject"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Domain" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="Content" abstract="true" eSuperTypes="#//failuretype/Domain">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="degreeOfCorruption" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Timing" abstract="true" eSuperTypes="#//failuretype/Domain">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="delayFactor" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Crash" abstract="true" eSuperTypes="#//failuretype/Domain"/>
    <eClassifiers xsi:type="ecore:EClass" name="Mode" abstract="true"/>
    <eClassifiers xsi:type="ecore:EClass" name="Transient" abstract="true" eSuperTypes="#//failuretype/Mode">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="One-time termination, or for the specified duration."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="duration" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Permanent" abstract="true" eSuperTypes="#//failuretype/Mode">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The time period up to which the error reoccurs is stored. When 1 permanently."/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="reoccurencePeriod" lowerBound="1"
          eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="Byzantine" abstract="true" eSuperTypes="#//failuretype/Mode">
      <eStructuralFeatures xsi:type="ecore:EAttribute" name="probabilityOfOccurence"
          lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EDouble"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SWInducedBitFailure" eSuperTypes="#//failuretype/SWFailure #//failuretype/Content"/>
    <eClassifiers xsi:type="ecore:EClass" name="SWLongCalcFailure" eSuperTypes="#//failuretype/SWFailure #//failuretype/Timing"/>
    <eClassifiers xsi:type="ecore:EClass" name="SWCrashFailure" eSuperTypes="#//failuretype/SWFailure #//failuretype/Crash"/>
    <eClassifiers xsi:type="ecore:EClass" name="SWPermanentCrashFailure" eSuperTypes="#//failuretype/SWFailure #//failuretype/Crash #//failuretype/Permanent">
      <eStructuralFeatures xsi:type="ecore:EReference" name="decoratedFailure" lowerBound="1"
          eType="#//failuretype/SWCrashFailure"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="SWTransientCrashFailure" eSuperTypes="#//failuretype/SWFailure #//failuretype/Crash #//failuretype/Transient">
      <eStructuralFeatures xsi:type="ecore:EReference" name="decoratedFailure" lowerBound="1"
          eType="#//failuretype/SWCrashFailure"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="HWInducedBitFailure" eSuperTypes="#//failuretype/HWFailure #//failuretype/Content"/>
    <eClassifiers xsi:type="ecore:EClass" name="HWLowerProcessingRateFailure" eSuperTypes="#//failuretype/HWFailure #//failuretype/Timing"/>
    <eClassifiers xsi:type="ecore:EClass" name="HWNAFailure" eSuperTypes="#//failuretype/HWFailure #//failuretype/Crash"/>
    <eClassifiers xsi:type="ecore:EClass" name="HWPermanentNAFailure" eSuperTypes="#//failuretype/HWFailure #//failuretype/Crash #//failuretype/Permanent">
      <eStructuralFeatures xsi:type="ecore:EReference" name="decoratedFailure" lowerBound="1"
          eType="#//failuretype/HWNAFailure"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="HWTransientNAFailure" eSuperTypes="#//failuretype/HWFailure #//failuretype/Crash #//failuretype/Transient">
      <eStructuralFeatures xsi:type="ecore:EReference" name="decoratedFailure" lowerBound="1"
          eType="#//failuretype/HWNAFailure"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LinkResponseFailure" eSuperTypes="#//failuretype/LinkFailure #//failuretype/Content"/>
    <eClassifiers xsi:type="ecore:EClass" name="LinkDelayFailure" eSuperTypes="#//failuretype/LinkFailure #//failuretype/Timing"/>
    <eClassifiers xsi:type="ecore:EClass" name="LinkArbitraryFailure" eSuperTypes="#//failuretype/LinkFailure #//failuretype/Byzantine">
      <eAnnotations source="http://www.eclipse.org/emf/2002/GenModel">
        <details key="documentation" value="The byzantine failure probability generates one of the stored failure types when triggered."/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore">
        <details key="constraints" value="noLinkArbitraryFailureAllowedForInternalPossibleFailuresDescription"/>
      </eAnnotations>
      <eAnnotations source="http://www.eclipse.org/emf/2002/Ecore/OCL">
        <details key="noLinkArbitraryFailureAllowedForInternalPossibleFailuresDescription"
            value="not (self.possibleFailures -> exists(self.oclIsTypeOf(LinkArbitraryFailure)))"/>
      </eAnnotations>
      <eStructuralFeatures xsi:type="ecore:EReference" name="possibleFailures" lowerBound="1"
          upperBound="-1" eType="#//failuretype/LinkFailure"/>
    </eClassifiers>
    <eClassifiers xsi:type="ecore:EClass" name="LinkCrashFailure" eSuperTypes="#//failuretype/LinkFailure #//failuretype/Crash"/>
    <eClassifiers xsi:type="ecore:EClass" name="LinkOmissionFailure" eSuperTypes="#//failuretype/LinkFailure #//failuretype/Crash #//failuretype/Transient">
      <eStructuralFeatures xsi:type="ecore:EReference" name="decoratedFailure" lowerBound="1"
          eType="#//failuretype/LinkCrashFailure"/>
    </eClassifiers>
  </eSubpackages>
</ecore:EPackage>
